# ===========================
# AicodeX Environment Variables
# ===========================
# Copy this file to .env and fill in the values

# Build Configuration
BUILD_TYPE=debug
# Options: debug, release, ci, nightly, hotfix

# Python Configuration
PYTHON_VERSION=3.11
VIRTUAL_ENV=.venv

# Rust Configuration  
RUST_VERSION=1.75
CARGO_HOME=${HOME}/.cargo

# TypeScript/Node Configuration
NODE_VERSION=20
NPM_CONFIG_REGISTRY=https://registry.npmjs.org

# Go Configuration
GO_VERSION=1.21
GOPATH=${HOME}/go

# Feature Flags
FEATURE_A_ENABLED=true
FEATURE_B_ENABLED=false

# Security / Secrets (DO NOT COMMIT REAL VALUES)
# SIGNING_KEY_ID=
# REGISTRY_TOKEN=
